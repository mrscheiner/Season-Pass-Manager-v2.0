{
  "packageManager": "npm@9.6.7",
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "bunx rork start -p 9ingb6hiqfsq4kgpjtixi --tunnel",
    "start-web": "bunx rork start -p 9ingb6hiqfsq4kgpjtixi --web --tunnel",
    "start:expo": "npx expo start --tunnel",
    "start:expo:clear": "npx expo start -c --tunnel",
    "start:expo:lan": "npx expo start --lan",
    "start:expo:lan:clear": "npx expo start -c --lan",
    "start-web:expo": "npx expo start --web --tunnel",
    "start-web:expo:clear": "npx expo start --web --clear --tunnel",
    "start-web:expo:lan": "npx expo start --web --lan",
    "start-web:expo:lan:clear": "npx expo start --web --clear --lan",
    "dev:clear": "npx expo start -c",
    "tail:logs": "tail -f ./start.log | rg '\\[Schedule Debug\\]|\\[Schedule\\]' || tail -f ./start.log",
    "watch-schedule": "node ./scripts/watch-and-reload.js",
    "assistant:open": "open -a \"Google Chrome\" \"https://chat.openai.com/\"",
    "assistant:cli": "node ./scripts/assistant-cli.js",
    "assistant:open-bookmark": "node ./scripts/open-bookmark.js",
    "fetch:panthers-schedule": "node ./scripts/fetchPanthersSchedule.js --teamId=13 --season=20252026",
    "fetch:panthers-espn": "node ./scripts/fetchEspnSchedule.js --url=https://www.espn.com/nhl/team/schedule/_/name/fla/seasontype/2 --season=20252026",
    "fetch:panthers-aggregate": "node ./scripts/aggregatePanthersSchedule.js --teamId=13 --season=20252026",
    "fetch:panthers-nhlcom": "node ./scripts/fetchNhlComSchedule.js --url=https://www.nhl.com/panthers/schedule --season=20252026",
    "fetch:panthers-team": "node ./scripts/fetchTeamSiteSchedule.js --url=https://www.floridapanthers.com/schedule --season=20252026",
    "fetch:panthers-ticketmaster": "node ./scripts/fetchTicketmasterSchedule.js --keyword=Florida%20Panthers --season=20252026",
    "fetch:tm-schedules": "node ./scripts/fetchAllLeaguesTicketmaster.js --season=20252026 --out=./constants/seasonPassSchedules_20252026_ticketmaster.ts",
    "start-web-dev": "DEBUG=expo* bunx rork start -p 9ingb6hiqfsq4kgpjtixi --web --tunnel",
    "make:recovery:from-json": "node ./scripts/recovery-from-json.js",
    "lint": "expo lint",
    "test:backfill": "node ./scripts/test-backfill.js",
    "watcher": "node ./dev/watcher/server.js",
    "start:api": "bun ./backend/server.ts",
    "dev": "node ./scripts/dev-startup.js",
    "dev:check": "node ./scripts/health-check.js",
    "dev:status": "node ./scripts/dev-status.js",
    "dev:restart": "node ./scripts/dev-restart.js",
    "git:watch": "node ./scripts/git-auto-sync.js",
    "git:sync": "git add -A && git commit -m \"Manual sync $(date)\" && git push"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hono/trpc-server": "^0.4.2",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@rork-ai/toolkit-sdk": "^0.2.51",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@tanstack/react-query": "^5.90.19",
    "@trpc/client": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@ungap/structured-clone": "^1.3.0",
    "expo": "~54.0.33",
    "expo-blur": "~15.0.8",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-document-picker": "^14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-mail-composer": "~15.0.8",
    "expo-print": "~15.0.8",
    "expo-router": "~6.0.23",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "hono": "^4.11.4",
    "jspdf": "^4.1.0",
    "lucide-react-native": "^0.475.0",
    "lz-string": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "superjson": "^2.2.6",
    "xlsx": "^0.18.5",
    "zod": "^4.3.5",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/cli": "^54.0.21",
    "@expo/ngrok": "^4.1.0",
    "@expo/webpack-config": "^19.0.1",
    "@types/react": "~19.1.10",
    "eslint": "^9.31.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.8.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "typescript"
      ]
    }
  },
  "overrides": {
    "expo-location": "~19.0.8"
  },
  "private": true
}
